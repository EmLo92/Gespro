		}
		
		dto.setIdEstatus( rs.getInt( COLUMN_ID_ESTATUS ) );
		if (rs.wasNull()) {
			dto.setIdEstatusNull( true );
		}
		
		dto.setIncidencia( rs.getInt( COLUMN_INCIDENCIA ) );
		if (rs.wasNull()) {
			dto.setIncidenciaNull( true );
		}
		
		reset(dto);
	}

