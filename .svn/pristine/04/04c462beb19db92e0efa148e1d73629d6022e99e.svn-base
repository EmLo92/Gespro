		dto.setIdUsuario( rs.getInt( COLUMN_ID_USUARIO ) );
		dto.setIdCliente( rs.getInt( COLUMN_ID_CLIENTE ) );
		dto.setFechaHora( rs.getTimestamp(COLUMN_FECHA_HORA ) );
		dto.setIdTipoCheck( rs.getInt( COLUMN_ID_TIPO_CHECK ) );
		if (rs.wasNull()) {
