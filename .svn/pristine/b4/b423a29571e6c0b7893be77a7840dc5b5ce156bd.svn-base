		if (fechaApertura != null) {
			_hashCode = 29 * _hashCode + fechaApertura.hashCode();
		}
		
		_hashCode = 29 * _hashCode + (fechaAperturaModified ? 1 : 0);
		if (fechaCierre != null) {
			_hashCode = 29 * _hashCode + fechaCierre.hashCode();
		}
		
		_hashCode = 29 * _hashCode + (fechaCierreModified ? 1 : 0);
		return _hashCode;
	}

